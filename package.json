    "husky": "^9.0.11",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.5",
    "turbo": "^2.0.6",
